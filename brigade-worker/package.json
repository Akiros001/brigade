    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.14",
    "chai": "^4.1.0",
    "mocha": "^3.4.2",
    "typedoc": "^0.8.0",
    "typescript": "^2.4.2"
  },
  "dependencies": {
    "ulid": "^0.2.0"
  }
}
